{% extends 'base.html.twig' %}

{% block body %}
    <section>
        {% for space in ves %}
            <div>
                <h1>{{ space.name }}</h1>
                <span>{{ space.type }}</span>
                <span>{{ space.hp }}</span>
                <span>{{ space.hpMax }}</span>
                <span>{{ space.damages }}</span>
            </div>
        {% endfor %}
    </section>
{% endblock %}
